<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
         http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="product" class="spring9.Product">
        <constructor-arg value="182"/>
        <constructor-arg value="Laptop"/>
        <constructor-arg value="80000"/>
    </bean>

    <bean id="purchase" class="spring9.Purchase">
        <constructor-arg value="1"/>
        <constructor-arg value="2025-08-02"/>
        <constructor-arg ref="product"/>
        
    </bean>

</beans>
